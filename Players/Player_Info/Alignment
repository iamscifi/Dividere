def alignment_token_check(neutral_tokens, good_tokens, evil_tokens):
  if neutral_tokens and good_tokens and evil_tokens > 5:
    if neutral_tokens >= good_tokens and evil_tokens:
      return neutral_path
    elif good_tokens > neutral_tokens and evil_tokens:
      return good_path
    else:
      return evil_path
  else:
    return new_player_path



def player_alignment(player_id):
  return type(player_id)

if float(player_alignment):
  return neutral_path
elif int(player_alignment):
  return good_path
elif str(player_alignment):
  return evil_path

